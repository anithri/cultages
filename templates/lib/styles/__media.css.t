@custom-media --breakpoint-small screen and (max-width: 40em);
@custom-media --breakpoint-not-small screen and (min-width: 40em);
@custom-media --breakpoint-medium screen and (max-width: 50em);
@custom-media --breakpoint-large screen and (min-width: 50em) and (max-width: 960px);
@custom-media --breakpoint-extra-large screen and (min-width: 960px);
@custom-media --breakpoint-not-large screen and (max-width: 50em);
